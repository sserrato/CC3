<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>


<div>
<table class="table table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th>User name</th>
      <th>First name</th>
      <th>Last name</th>
      <th>City</th>
      <th>Phone</th>
    </tr>
    <tbody>
        <% @users.each do |w| %>
          <tr>
            <td><%=w.id %></td>
            <td><%=w.email %></td>
            <td><%=w.first_name %></td>
            <td><%=w.last_name %></td>
            <td><%=w.city %></td>
            <td><%=w.phone %></td>
          </tr>
        	<% end %>
    </tbody>
</table>
</div>
